<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <!-- Cron Job for automatic state change of contracts -->
    <data noupdate="1">
        <record id="sequence_ct" model="ir.sequence">
            <field name="name">Sequence contracts</field>
            <field name="code">sequence_subscription_contracts</field>
            <field name="active">True</field>
            <field name="prefix">CT/%(month)s/%(y)s/</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">3</field>
            <field name="implementation">standard</field>
    </record>
    </data>
</odoo>
